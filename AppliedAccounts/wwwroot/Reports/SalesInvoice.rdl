  <DataSets>
    <DataSet Name="ds_SaleInvoice">
      <Query>
        <DataSourceName>DataSet3</DataSourceName>
        <CommandText>SELECT [B2].[TranID],

[B1].[Vou_No], 
[B1].[Vou_Date], 
[C].[Title] AS [Company], 
[E].[Title] AS [Employee], 
[P].[Title] AS [Project], 
[B1].[Ref_No], 
[B1].[Inv_No], 
[B1].[Inv_Date], 
[B1].[Pay_Date], 
[B1].[Description], 
[B2].[Sr_No], 
[I].[Title] AS [Inventory], 
[B2].[Batch], 
[B2].[Qty] AS [Qty],
[B2].[Rate], 
[B2].[Qty] * [B2].[Rate] AS [Amount], 
[T].[Rate] AS [Tax_Rate], 
([B2].[Qty] * [B2].[Rate]) * [T].[Rate] AS [Tax_Amount], 
([B2].[Qty] * [B2].[Rate]) + (([B2].[Qty] * [B2].[Rate]) * [T].[Rate]) AS [Net_Amount], 
[B2].[Description] AS [Remarks], [C].[Address1], [C].[Address2], [C].[City] || ' ' || [C].[State] || ' ' || [C].[Country] AS [Address3], 
[C].[Phone] 
FROM [BillReceivable] [B1] 
LEFT JOIN[BillReceivable2] [B2] ON[B2].[TranID] = [B1].[ID] 
LEFT JOIN[Customers] [C] ON[C].[ID] = [B1].[Company] 
LEFT JOIN[Employees] [E] ON[E].[ID] = [B1].[Employee] 
LEFT JOIN[Project] [P] ON[P].[ID] = [B2].[Project] LEFT JOIN[Inventory] [I] ON[I].[ID] = [B2].[Inventory] LEFT JOIN[Taxes] [T] ON[T].[ID] = [B2].[Tax]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
